<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Version>1.0.0</Version>
    <Copyright>Infomatiq Limited, Aled Morris</Copyright>
    <Authors>Dror Gluska, Lojemiru</Authors>
    <Company />
    <Description>A fork of Dror Gluska's RTree library, pared down to two dimensions and updated to use modern .NET Core.</Description>
    <PackageProjectUrl>https://github.com/Lojemiru/2D-RTree</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Lojemiru/2D-RTree</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>rtree, spatial index</PackageTags>
    <PackageId>2D-RTree</PackageId>
    <Product>2D-RTree</Product>
  </PropertyGroup>

</Project>
